<h1>Recipes#index</h1>
<p>Find me in app/views/recipes/index.html.erb</p>
<h4><%= link_to("New Recipe", "/recipes/new") %></h4>
<% @recipes.each do |recipe| %>
<p><%= link_to(recipe.name, "/recipe/#{recipe.id}") %> -
   <%= link_to("Edit", "/recipes/edit/#{recipe.id}") %> -
   <%= link_to("Delete", "/recipes/delete/#{recipe.id}", :method => :delete) %> -
</p>
  <% end %>